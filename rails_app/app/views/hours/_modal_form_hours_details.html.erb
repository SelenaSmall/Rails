<div class="modal-dialog reorder-modal-form">
<div class="modal-content">
	<%= bootstrap_form_for([@organisation, @venue, @hour], remote: true) do |f| %>
<div class="modal-header">
        <h4 class="modal-title">Hours Details</h4>
</div>

<div class="modal-body">
  <%= render "hours/details", venue: @venue.hours %>
</div>

<div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal" data-behaviour="form-clear">Cancel</button>
</div>
<% end %>
</div>
</div>
