<h1>Bookings for organisation: <% @organisations.each do |organisation| %><%= organisation.name %></h1>
  	<h2><%= GetDate.generate %> Tabs available: <%= organisation.number_of_tabs %></h2>
	<% end %>

<table>
  <thead>
    <tr>
      <th></th>
      <% 15.times do |count| %>
      <th>Time</th>
      <% end %>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  	<% @organisations.each do |organisation| %>

  	<% organisation.tables.each do |table| %>
  	<tr>
  		<td>Tab <%= table.table_number %></td>
  	</tr>
  	<% end %>
  	<% end %>
  </tbody>
 </table>

<%= link_to 'Back to organisations', organisations_path %>
<%= link_to 'View Customers', customers_path %>
