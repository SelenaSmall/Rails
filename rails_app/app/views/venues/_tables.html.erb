<div class="bordered-box">

<table class="table">
  <thead>
    <tr>
      <th>Tab</th>
      <th>Section</th>
      <th>Min Covers</th>
      <th>Max Covers</th>
      <th>Booking</th>
     </tr>
  </thead>
  <tbody>
    <% @venue.tables.each do |table| %>
      <tr>
        <div id="tables"></div>
          <td><%= table.tab %></td>
          <td><%= table.section %></td>
          <td><%= table.min_covers %></td>
          <td><%= table.max_covers %></td>
          <td><%= table.booking_order %></td>
          <td><%= link_to 'Edit', edit_venue_table_path(@venue, table) %> </td>
          <td><%= link_to 'Delete', [@venue, table], method: :delete, data: { confirm: 'Are you sure?' }%> </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to new_venue_table_path(@venue), 
  remote: true, id: "new-item-link", class: "btn btn-sm btn-default round" do %> New
<%end%>

</div>