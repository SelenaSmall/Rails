<div class="modal-dialog reorder-modal-form">
<div class="modal-content">
	<%= bootstrap_form_for([venue.organisation, venue], remote: true) do |f| %>
<div class="modal-header">
        <h4 class="modal-title">sections</h4>
</div>

<div class="modal-body">
  <table>
    <th>
  <th class="field"><%= f.text_field :sections %></th>
  <th class="field"><%= f.label :sections_group %></th>
  <th class="field"><%= f.label :turn_time %></th> 
    </th>
  </table>
</div>

<div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal" data-behaviour="form-clear">Cancel</button>
        <%= f.submit class: "btn btn-primary", data: { trigger: 'loading' } %>
</div>
<% end %>
</div>
</div>
