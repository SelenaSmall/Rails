<p id="notice"><%= notice %></p>
<body>
 <div class="main-container">
  <h1><center>Grid View</center></h1>
   
   <!--this is not actually what I want to do. I need to find a way to access the opening info for 1 specific day and venue-->
  <table class="table">
   <thead>
    <tr>
     <th>Tab#</th>
     <th>
      <% @venue.hours.each do |hour| %>
       <%= hour.open_time %>
      <% end %>    
     </th>   
    </tr>  
   </thead>
   <tbody>
    <% @venue.tables.each do |table| %>
     <tr>
      <td><%= table.tab %></td>
      <td>
       <%= link_to new_venue_booking_path(@venue),
        remote: true, id: "new-item-link", class: "btn btn-sm btn-default round" do %> New
       <%end%>
      </td>
     </tr>
    <%end%> 
   </tbody>
  </table>
  <!-- Modal form / New Channel Group -->
  <div class="modal fade" id='new-booking-modal-form' role="dialog"></div>
  <!-- Modal form / New Channel Group -->
 </div>
</body>