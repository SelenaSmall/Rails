<p id="notice"><%= notice %></p>

<h1> BOOKINGS</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Start time </th>
      <th>Tab</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bookings.each do |booking| %>
      <tr>
        <td><%= booking.name %></td>
        <td><%= booking.phone %></td>
        <td><%= booking.email %></td>
        <td><%= booking.start_time %></td>
        <td><%= booking.tab_name %></td>
        <td><%= link_to 'Show', [@user, @venue, @booking] %></td>
        <td><%= link_to 'Edit', edit_user_venue_booking_path(@user, @venue, @booking) %></td>
        <td><%= link_to 'Destroy', [@user, @venue, @booking], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New Booking', new_user_venue_booking_path %>
<%= link_to 'Back to Venues', user_venues_path %>

