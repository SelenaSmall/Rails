<% if user_signed_in? %>
 <% if current_user.try(:admin?) %>

  <div class="container">
   <div class="row">
    <h3>Users</h3>
    <div class="column">
     <table class="table">
      <tbody>
       <% @users.each do |user| %>
        <tr>
         <%= render user %>
        </tr>
       <% end %>
      </tbody>
     </table>
    </div>
   </div>
  </div>
  <% else %>
   <%= render 'visitors/home' %>
 <% end %>
<% end %>